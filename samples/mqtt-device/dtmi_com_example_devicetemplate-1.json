{
	"@context": "dtmi:dtdl:context;2",
	"@id": "dtmi:com:example:DeviceTemplate;1",
	"@type": "Interface",
	"displayName": "Sample MQTT Device",
	"description": "Sample interface describing a MQTT device.",
	"contents": [
		{
			"@type": "Property",
			"name": "sdkInfo",
			"displayName": "SDK info",
			"schema": "string"
		},
		{
			"@type": [ "Property", "TimeSpan" ],
			"name": "interval",
			"schema": "integer",
			"writable": true,
			"unit": "second"
		},
		{
			"@type": [ "Telemetry", "Temperature" ],
			"name": "temp",
			"displayName": "Temperature",
			"schema": "double",
			"unit": "degreeCelsius"
		},
		{
			"@type": "Command",
			"name": "echo",
			"request": {
				"@type": "CommandPayload",
				"name": "commandInput",
				"schema": "string"
			},
			"response": {
				"@type": "CommandPayload",
				"name": "commandOutput",
				"schema": "string"
			}
		},
		{
			"@type": "Command",
			"name": "getRuntimeStats",
			"request": {
				"@type": "CommandPayload",
				"name": "diagnosticsMode",
				"schema": {
					"@type": "Enum",
					"enumValues": [
						{
							"name": "minimal",
							"enumValue": 0
						},
						{
							"enumValue": 1,
							"name": "complete"
						},
						{
							"name": "full",
							"enumValue": 2
						}
					],
					"valueSchema": "integer"
				}
			},
			"response": {
				"@type": "CommandPayload",
				"name": "diagnosticResults",
				"schema": {
					"@type": "Map",
					"mapKey": {
						"name": "key",
						"schema": "string"
					},
					"mapValue": {
						"name": "val",
						"schema": "string"
					}
				}
			}
		}
	]
}